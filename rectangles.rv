//draws rectangles//
main () {
int a
int b
print ("Height: ")
int height = 0
input ("%d",&height)
print ("Width: ")
int width = 0
input ("%d",&width)
for (a = 0; a != height; a++) {

    for (b = 0; b != width; b++ ) {

        if (a == 0 || a== height-1 ) {
            print ("+")
       }else{
            if(b == 0 || b == width-1){
                print ("+")
            }else{
                print (" ")
            }
       }
    }

    print ("\n")
}}